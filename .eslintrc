{
    "extends": "eslint-config-egg",
    "rules": {
        "linebreak-style": "off"
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": { "var": 2, "let": 2, "const": 3 }
            }
        ]
    }
}
